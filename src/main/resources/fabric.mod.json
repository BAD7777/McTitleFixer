{
  "schemaVersion": 1,
  "id": "titlefixer",
  "version": "${version}",
  "name": "Title Fixer",
  "description": "Makes the Minecraft title fit the screen at all times.",
  "authors": [
    "TheGameratorT"
  ],
  "contributors": [
    "moehreag"
  ],
  "contact": {
    "sources": "https://github.com/TheGameratorT/McTitleFixer",
    "issues": "https://github.com/TheGameratorT/McTitleFixer/issues"
  },
  "license": "GPL-3.0",
  "icon": "assets/titlefixer/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.thegameratort.titlefixer.TitleFixer"
    ],
    "modmenu": [
      "com.thegameratort.titlefixer.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    {
      "config": "titlefixer.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.17.3",
    "minecraft": ">=1.21.9 <=1.21.10",
    "cloth-config2": ">=19.0.147"
